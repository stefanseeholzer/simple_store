<h1>Products</h1>

<ul>
    <% @products.each do |product| %>
    <li>
        Title: <%= product.title %><br>
        Description: <%= product.description %><br>
        Price: <%= number_to_currency(product.price) %><br>
        Stock Quantity: <%= product.stock_quantity %>
    </li>
    <br>
    <% end %>
</ul>